{
  "name": "time-tracking-service",
  "version": "1.0.0",
  "description": "Microservice to help employees to view their worked hours.",
  "main": "dist/index.js",
  "scripts": {
    "prestart": "npm run build",
    "start": "node -r ts-node/register/transpile-only -r tsconfig-paths/register .",
    "build": "tsc --p tsconfig.build.json",
    "pretty-quick": "npx pretty-quick --staged --verbose --pattern \"**/*.*(js|ts|json)\"",
    "start:dev": "ts-node-dev -r tsconfig-paths/register --respawn --transpile-only --ignore-watch node_modules src/index.ts --trace-warnings",
    "test": "jest -i",
    "test:watch": "jest -i --watch",
    "test:cucumber": "cucumber-js",
    "test:verbose": "jest --verbose -i",
    "test:coverage": "jest --coverage -i"
  },
  "author": "Denis Slapelis",
  "dependencies": {
    "@aws-sdk/client-dynamodb": "^3.538.0",
    "@aws-sdk/client-ses": "^3.535.0",
    "@aws-sdk/client-ssm": "^3.525.0",
    "@aws-sdk/util-dynamodb": "^3.538.0",
    "cors": "^2.8.5",
    "dayjs": "^1.11.10",
    "dotenv": "^16.4.5",
    "express": "^4.18.3",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "reflect-metadata": "^0.2.1",
    "tsconfig-paths": "^4.2.0",
    "tsyringe": "^4.8.0",
    "twilio": "^4.23.0",
    "winston": "^3.11.0"
  },
  "devDependencies": {
    "@cucumber/cucumber": "^10.3.1",
    "@tsconfig/node20": "^20.1.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.11",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/lodash": "^4.14.202",
    "@types/node": "^20.10.8",
    "@types/winston": "^2.4.4",
    "axios": "^1.6.8",
    "cross-env": "^7.0.3",
    "jest": "^29.7.0",
    "prettier": "^3.1.1",
    "pretty-quick": "^3.1.3",
    "ts-jest": "^29.1.2",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
